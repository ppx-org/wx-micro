
.prodView {
  width:420rpx;display:flex;flex-direction:column;justify-content:space-between;margin:3rpx;
}

.actionView {
  width:420rpx;display:flex;justify-content:space-around;align-items:center;
}

.action {
  border:1px solid #EEEEEE;width:50rpx;height:50rpx;font-size:30rpx;text-align:center;
}

.prodTitleText {
  font-size:24rpx;height:64rpx;overflow:hidden;text-overflow:ellipsis;display:-webkit-box !important; -webkit-line-clamp:2;-webkit-box-orient:vertical
}

.finish {
  width:100rpx;background-color:#FE7506;color:white;display:flex;justify-content:space-around;align-items:center;
}