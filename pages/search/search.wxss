

.searchView {display:flex;justify-content:space-between;height:56rpx;position:relative;z-index:2000;background-color:white;padding:8rpx 120rpx;
  font-size:28rpx;border-bottom: 1px solid #EEEEEE;
}
.searchView input {border-bottom:1px solid gray;width:360rpx;height:56rpx;}
.searchView button {display:flex;background-color:#FE7506;color:white;margin:0rpx;height:56rpx;}

.searchItemTitle {display:flex;align-items:center;font-size:28rpx;}
.searchItem {display:flex;flex-wrap:wrap;}
.searchItem text {white-space:nowrap;background-color:Gainsboro;padding:20rpx 20rpx;border-radius:20rpx;margin-left:20rpx;margin-bottom:20rpx;}
.searchHistory {display:flex;justify-content:space-between;margin:20rpx 20rpx;height:40rpx;}
.searchHistory image {width:40rpx;height:40rpx}
.seachRecommend {display:flex;justify-content:space-between;margin:0rpx 20rpx 20rpx 20rpx;height:40rpx;}


.resultTitle {display:flex;justify-content:space-around;height:70rpx;align-items:center;font-size:28rpx;
  border-bottom:1px solid #EEEEEE;position:relative;z-index:2000;background-color:white;
}
.openFilter {display:flex;justify-content:space-around;align-items:center;}
.resultTitle image {width:28rpx;height:28rpx;}
.deliveryView {position:relative;background-color:white;}
.deliveryItem {height:70rpx;display:flex;justify-content:space-between;padding:0rpx 36rpx;align-items:center;font-size:28rpx;}
.deliveryBorderTop {border-top:1rpx solid #EEEEEE;}

.prodView {position:absolute;top:118rpx; padding:0;display:flex;justify-content:space-between;flex-wrap:wrap;background-color:#EEEEEE}

.arrow-bottom {font-size:0;line-height:0;border-width:10rpx;border-color:black;border-bottom-width:0;  
  border-style:dashed;border-top-style:solid;border-left-color:transparent;border-right-color:transparent;
  float:right;width:3rpx;height:3rpx;margin-top:10rpx;
}

.maskView {position:fixed;top:0;left:0;width:100%;height:100%;background-color:black;opacity: 0.5;}
.filterView {font-size:22rpx;flex-direction:column; position:fixed;top:0;
  background-color:white;z-index:3000;width:560rpx;height:100%; transition:right .3s;
}
.filterTitle {background-color:Gainsboro;color:LightSlateGray;padding:10rpx 20rpx;}
.filterBlockTitle {display:flex;justify-content:space-between;margin:0rpx 20rpx;height:60rpx;align-items:center;}
.filterBlockTitle image {width:30rpx;height:30rpx}
.filterLine {border-top:1px solid Gainsboro;width:100%;margin-top:10rpx}
.filterBlock {display:flex;flex-wrap:wrap;margin:0 20rpx 10rpx 20rpx;justify-content:space-between;}
.filterBlock view  {
  display:flex;align-items:center;justify-content:space-around;
  width:152rpx;height:62rpx;background-color:#EEEEEE;padding:8rpx;margin-bottom:10rpx;border-radius:20rpx;
}

.filterBlock text {
 text-align:center;
 overflow:hidden;text-overflow:ellipsis;display:-webkit-box !important; -webkit-line-clamp:2;-webkit-box-orient:vertical
}


.toTop {
  opacity:0.7;border-radius:50rpx;background-color:white;position:fixed;bottom:10rpx;right:10rpx;width:60rpx;height:60rpx;border:solid 1px gray;
}
.toTop image {width:40rpx;height:40rpx;padding:10rpx}
